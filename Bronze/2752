//숫자 세 개가 주어졌을 때, 오름차순으로 정렬하기
//입력: a, b, c
//출력: a ,b, c 오름차순대로

// if - else if
#include <iostream>
using namespace std;

int main(void)
{
	int a, b, c;
	cin >> a >> b >> c;

	if (a > b && a > c) {
		if (b > c)
			cout << c << " " << b << " " << a;
		else if (c > b)
			cout << b << " " << c << " " << a;
	}
	else if (b > a && b > c) {
		if (a > c)
			cout << c << " " << a << " " << b;
		else if (c > a)
			cout << a << " " << c << " " << b;
	}
	else if (c > a && c > b) {
		if (a > b)
			cout << b << " " << a << " " << c;
		else if (b > a)
			cout << a << " " << b << " " << c;
	}
	return 0;

	}
	
	
	
	
// if -else 위주
	
	#include <iostream>
using namespace std;

int main(void)
{
	int a, b, c;
	cin >> a >> b >> c;

	if (a > b && a > c) {
		if (b > c)
			cout << c << " " << b << " " << a;
		else
			cout << b << " " << c << " " << a;
	}
	else if (b > a && b > c) {
		if (a > c)
			cout << c << " " << a << " " << b;
		else
			cout << a << " " << c << " " << b;
	}
	else {
		if (a > b)
			cout << b << " " << a << " " << c;
		else
			cout << a << " " << b << " " << c;
	}
	return 0;

	}



